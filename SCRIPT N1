create database CATRIEL_BD;
USE CATRIEL_BD;

CREATE TABLE ESTUDIANTES (
ID INT PRIMARY KEY,
NOMBRE VARCHAR (50),
FECHANACIMIENTO DATE
);

SELECT * FROM ESTUDIANTES;
INSERT INTO ESTUDIANTES(ID,NOMBRE,FECHANACIMIENTO)
VALUES (1,'VALENTINO','2006-11-10'),(2,'SANTIAGO','2006-11-19');
DELETE FROM ESTUDIANTES WHERE ID=2;

CREATE TABLE MATERIALES(
CODIGO INT PRIMARY KEY IDENTITY(1,1),
NOMBRE VARCHAR (50)
);

INSERT INTO MATERIALES (NOMBRE) VALUES 
('MANTENIMIENTO III'),('PROGRAMACIÃ“N');

SELECT * FROM MATERIALES;
DELETE FROM MATERIALES WHERE CODIGO=3;
DELETE FROM MATERIALES WHERE CODIGO=4;


CREATE TABLE INSCRIPCION (
ESTUDIANTEID INT,
MATERIACODIGO INT,
FOREIGN KEY (ESTUDIANTEID) REFERENCES ESTUDIANTES(ID),
FOREIGN KEY (MATERIACODIGO) REFERENCES MATERIALES(CODIGO)
);

SELECT * FROM INSCRIPCION;
